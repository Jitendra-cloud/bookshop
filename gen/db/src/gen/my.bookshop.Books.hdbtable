COLUMN TABLE my_bookshop_Books (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  ID INTEGER NOT NULL,
  title NVARCHAR(111),
  descr NVARCHAR(1111),
  stock INTEGER,
  price DECIMAL(9, 2),
  currency_code NVARCHAR(3),
  authors_ID INTEGER,
  PRIMARY KEY(ID)
)